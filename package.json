{
  "name": "selfi-bot",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "prisma": {
    "seed": "node --loader tsx/esm prisma/seed.ts"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src/**/*.ts",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "test": "vitest"
  },
  "dependencies": {
    "@fal-ai/client": "^1.2.3",
    "@grammyjs/auto-retry": "^1.1.1",
    "@grammyjs/runner": "^2.0.3",
    "@prisma/client": "5.22.0",
    "aws-sdk": "2.1692.0",
    "dotenv": "16.4.7",
    "fastify": "4.29.0",
    "grammy": "1.34.1",
    "pino": "8.21.0",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@types/node": "20.17.16",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "eslint": "8.57.1",
    "prettier": "3.4.2",
    "prisma": "5.22.0",
    "tsx": "4.19.2",
    "typescript": "5.7.3",
    "vitest": "1.6.0"
  }
}